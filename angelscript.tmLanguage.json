{
    "_license": "This file is a modified version of a file from https://github.com/Hazelight/vscode-unreal-angelscript, originally licensed under the MIT License.",
    "name": "hlsl",
    "scopeName": "source.hlsl",
    "patterns": [
        {
            "name": "keyword.declaration.hlsl",
            "match": "\\b(class|struct|delegate|event|import|enum|namespace)\\b"
        },
        {
            "name": "keyword.statement.hlsl",
            "match": "\\b(for|if|return|continue|break|default|else|while|case|switch|fallthrough)\\b"
        },
        {
            "name": "keyword.type.hlsl",
            "match": "\\b(void|auto|const|property|final|override|mixin|local)\\b"
        },
        {
            "name": "keyword.code.hlsl",
            "match": "\\b(Cast|Super)\\b"
        },
        {
            "name": "keyword.code.hlsl.NAME_None.name",
            "match": "\\bNAME_None\\b"
        },
        {
            "name": "keyword.code.hlsl.null.reference",
            "match": "\\bnull\\b"
        },
        {
            "name": "keyword.code.hlsl.this.reference",
            "match": "\\bthis\\b"
        },
        {
            "name": "keyword.code.hlsl.true.boolean",
            "match": "\\btrue\\b"
        },
        {
            "name": "keyword.code.hlsl.false.boolean",
            "match": "\\bfalse\\b"
        },
        {
            "name": "keyword.access.hlsl",
            "match": "\\b(private|protected|access)\\b"
        },
        {
            "name": "storage.type.primitive.hlsl",
            "match": "\\b(int|float|double|bool|int32|int64|int16|int8|uint32|uint64|uint16|uint8|float32|float64)\\b"
        },
        {
            "match": "::",
            "name": "punctuation.separator.namespace.access.hlsl"
        },
        {
            "name": "meta.preprocessor.hlsl",
            "begin": "^\\s*#",
            "end": "(?=\\n)"
        },
        {
            "begin": "f\"",
            "beginCaptures": {
                "0": {
                    "name": "keyword.operator.quantifier.regexp"
                }
            },
            "end": "\"",
            "endCaptures": {
                "0": {
                    "name": "keyword.operator.quantifier.regexp"
                }
            },
            "name": "string.quoted.double.hlsl",
            "patterns": [
                {
                    "include": "#string_escaped_char"
                },
                {
                    "match": "{{",
                    "name": "string.quoted.double.hlsl"
                },
                {
                    "match": "}}",
                    "name": "string.quoted.double.hlsl"
                },
                {
                    "begin": "{",
                    "beginCaptures": {
                        "0": {
                            "name": "keyword.formatexpression.hlsl"
                        }
                    },
                    "patterns": [
                        {
                            "include": "source.hlsl"
                        }
                    ],
                    "name": "meta.embedded.expression.hlsl",
                    "end": "=?\\s*(:(.?[=<>^])?[0-9dxXbconeEfFgG=,+-\\.^%#\\s]*)?(}|(?=\")|(?=\n))",
                    "endCaptures": {
                        "0": {
                            "name": "keyword.formatexpression.hlsl"
                        }
                    }
                }
            ]
        },
        {
            "include": "#comments"
        },
        {
            "include": "#numbers"
        },
        {
            "include": "#operators"
        },
        {
            "include": "#strings"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "name": "comment.block.hlsl",
                    "begin": "(\\/\\*)",
                    "beginCaptures": {
                        "1": {
                            "name": "punctuation.definition.comment.begin.hlsl"
                        }
                    },
                    "end": "(\\*\\/)",
                    "endCaptures": {
                        "1": {
                            "name": "punctuation.definition.comment.end.hlsl"
                        }
                    }
                },
                {
                    "name": "comment.line.double-slash.hlsl",
                    "begin": "(\\/\\/)",
                    "beginCaptures": {
                        "1": {
                            "name": "punctuation.definition.comment.hlsl"
                        }
                    },
                    "end": "(?=\\n)",
                    "patterns": [
                        {}
                    ]
                }
            ]
        },
